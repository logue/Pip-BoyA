    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "electron": "^11.0.2",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-vuetify": "^0.6.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-strict-vue": "^1.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "fibers": "^5.0.0",
    "fontsource-noto-sans-jp": "^3.0.10",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "google",
      "vuetify",
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vuetify",
      "strict-vue"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "htmlWhitespaceSensitivity": "ignore",
          "semi": true,
          "singleQuote": true,
          "trailingComma": "es5",
          "bracketSpacing": false,
          "printWidth": 80
        }
      ],
      "strict-vue/require-jsdoc": "error",
      "vue/no-deprecated-v-bind-sync": "off",
      "vuetify/no-deprecated-classes": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/logue/Pip-BoyA/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://www.nexusmods.com/fallout76/mods/697",
  "keywords": [
    "fo76 ",
    "fallout-76",
    "mappalachia",
    "vue",
    "vuetify",
    "vuelayers",
    "electron"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/Pip-BoyA.git"
  }

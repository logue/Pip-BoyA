{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "dist",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "ol": [
        "node_modules/@hanreev/types-ol/ol"
      ],
      "ol/*": [
        "node_modules/@hanreev/types-ol/ol/*"
      ]
    },
    "strict": false,
    "skipLibCheck": true,
    "target": "esnext",
    "types": [
      "webpack-env",
      "vuetify"
    ],
    "typeRoots": [
      "src/types/"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
,   "src/preload.ts"
  ]
}
